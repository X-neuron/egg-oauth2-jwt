{
  "name": "x-plat",
  "version": "1.0.0",
  "description": "my openApi project",
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "egg": "2.25.0",
    "egg-cors": "^2.2.3",
    "egg-oauth2-server": "^2.2.6",
    "egg-parameters": "^1.1.2",
    "egg-redis": "^2.4.0",
    "egg-scripts": "^2.11.1",
    "egg-sequelize": "^5.2.0",
    "egg-validate": "^2.0.2",
    "lodash": "^4.17.15",
    "mariadb": "^2.1.3",
    "moment": "^2.24.0",
    "nanoid": "^2.1.7",
    "node-forge": "^0.9.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "autod": "^3.1.0",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.14.0",
    "egg-mock": "^3.24.2",
    "eslint": "^6.7.1",
    "eslint-config-egg": "^7.5.1",
    "factory-girl": "^5.0.4",
    "sequelize-cli": "^5.5.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "autod": "autod",
    "start": "egg-scripts start --daemon --title=egg-server-eggjwt",
    "stop": "egg-scripts stop --title=egg-server-eggjwt",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && NODE_ENV=test npx sequelize db:migrate && npm run cov",
    "migrate-up": "npx sequelize db:migrate",
    "migrate-undo": "npx sequelize db:migrate:undo"
  },
  "ci": {
    "version": "10"
  },
  "eslintIgnore": [
    "coverage",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "files": [
    "lib",
    "index.js"
  ],
  "author": "x-neuron",
  "private": true
}
